{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OddEyes.ai",
  "version": "1.5.1",
  "identifier": "com.oddeyes.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "OddEyes.ai",
        "width": 1400,
        "height": 900,
        "minWidth": 1100,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": "default-src 'self' tauri: asset:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' asset: data:; font-src 'self' data:; connect-src 'self' ipc: https://api.openai.com https://api.anthropic.com https://generativelanguage.googleapis.com https://github.com https://*.githubusercontent.com https://objects.githubusercontent.com; media-src 'self' data: blob:; object-src 'none'; base-uri 'self'; frame-ancestors 'none'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDYwQTU4MDM4RDY0OEM2OTAKUldTUXhraldPSUNsWUowQ1NzOGdIeG1xdjFtZjR1VmFpYXYwSW5LdHUzS2VDSVlUVGJXY2hSQmMK",
      "endpoints": [
        "https://github.com/Hyunsang-coder/translation-editor/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}